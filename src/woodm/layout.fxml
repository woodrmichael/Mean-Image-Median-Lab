<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>


<HBox prefHeight="365.0" prefWidth="552.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="woodm.Controller">
   <children>
      <Button fx:id="saveButton" mnemonicParsing="false" onAction="#save" text="Save as" />
      <Button fx:id="addImageButton" mnemonicParsing="false" onAction="#inputImage" prefHeight="25.0" prefWidth="105.0" text="Add Image" />
      <ScrollPane fx:id="scrollPane" prefHeight="365.0" prefWidth="239.0">
         <content>
            <HBox fx:id="imageContainer" prefHeight="281.0" prefWidth="237.0" />
         </content>
      </ScrollPane>
   </children>
</HBox>
